{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "types": ["node", "bun-types"]
  },
  "include": ["*.ts", "tests/**/*.ts", "models/cfAiGateway.ts"],
  "exclude": ["node_modules", "dist"]
}
